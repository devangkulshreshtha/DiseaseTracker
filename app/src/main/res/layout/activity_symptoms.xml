<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffffff">
    <include
        android:id="@+id/tool_bar2"
        layout="@layout/tool_bar" />
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@id/tool_bar2"
        android:layout_marginLeft="16.0dip"
        android:layout_marginRight="16.0dip"
        android:layout_marginTop="10.0dip"
        android:text="What seems to be the problem?"
        android:textColor="@color/accent"
        android:textSize="24.0sp"
        android:textStyle="italic" />
    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView2"
        android:layout_alignLeft="@id/textView2"
        android:layout_alignRight="@id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@id/textView2"
        android:layout_marginTop="24.0sp"
        android:text="Choose problems you are suffering from and feel might be the cause of your bad health."
        android:textSize="16.0sp" />
    <View
        android:id="@+id/view"
        android:layout_width="fill_parent"
        android:layout_height="1.0dip"
        android:layout_below="@id/textView15"
        android:layout_marginLeft="16.0dip"
        android:layout_marginRight="16.0dip"
        android:layout_marginTop="20.0sp"
        android:background="@color/divider" />
    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/view"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@id/view"
        android:layout_marginTop="16.0dip"
        android:text="PICK UP TO 5 PROBLEMS"
        android:textColor="@color/secondary_text"
        android:textSize="16.0dip"
        android:textStyle="bold" />
    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@id/view"
        android:layout_marginBottom="16.0dip"
        android:background="#ffffff"
        android:text="Next"
        android:textStyle="italic"
        android:textColor="@color/accent" />
    <View
        android:id="@+id/view2"
        android:layout_width="fill_parent"
        android:layout_height="1.0dip"
        android:layout_above="@id/button"
        android:layout_marginBottom="16.0dip"
        android:layout_marginLeft="16.0dip"
        android:layout_marginRight="16.0dip"
        android:background="@color/divider" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@id/view2"
        android:layout_below="@id/textView16"
        android:layout_marginTop="16.0dip">
        <ListView
            android:id="@+id/listView"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="16.0dip"
            android:layout_marginRight="16.0dip" />

    </LinearLayout>

</RelativeLayout>